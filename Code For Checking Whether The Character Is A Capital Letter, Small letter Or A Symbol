#include <iostream>
using namespace std;
int main()
{
	char a;
	cout<<"Enter any character:";
	cin>>a;
	if (a>=65 && a<=69)
	cout<<"Character is a capital letter";
	else if (a>=97 && a<=122)
	cout<<"Character is a small letter";
	else if (a>=48 && a<=57)
	cout<<"Character is a digit";
	else if ((a>=0 && a<=47) || (a>=58 && a<=64) || (a>=91 && a<=96) || (a>=123 && a<=127))
	cout<<"Character is a symbol";
	
}
