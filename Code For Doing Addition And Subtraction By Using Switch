#include <iostream>
using namespace std;
int main()
{
	int x, y;
	cout<<"Enter value of x:";
	cin>>x;
	cout<<"Enter value of y:";
	cin>>y;
	cout<<"Enter the operator:";
	char oper;
	cin>>oper;
	switch (oper)
	{
		case '+':
		cout<<"Add of "<<x<<" and "<<y<<" is "<<x+y;
		break;
		case '-':
		cout<<"Subtract of "<<x<<" and "<<y<<" is "<<x-y;
		break;
	}
}
