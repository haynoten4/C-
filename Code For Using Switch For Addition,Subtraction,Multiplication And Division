#include <iostream>
using namespace std;
int main()
{
	int x, y;
	char oper;
	cout<<"Enter value for x:";
	cin>>x;
	cout<<"Enter value for y:";
	cin>>y;
	cout<<"Press + for addition"<<endl;
	cout<<"Press - for subtraction"<<endl;
	cout<<"Press * for multiplication"<<endl;
	cout<<"Press / for division"<<endl;
	cin>>oper;
	switch (oper)
	{
		case '+':
			cout<<"Add of "<<x<<" and "<<y<<" is "<<x+y;
			break;
		case '-':
			cout<<"Subtract of "<<x<< " and"<<y<<" is "<<x-y;
			break;
		case '*':
			cout<<"Multiply of "<<x<<" and "<<y<<" is "<<x*y;
			break;
		case '/':
			cout<<" Divide of "<<x<<" and "<<y<<" is "<<x/y;
			break;
	}
}
